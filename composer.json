{
  "name": "zohaib/generate-footer",
  "description": "Laravel package to generate a customizable footer",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "footer", "blade", "generator", "ui"],
  "autoload": {
    "psr-4": {
      "Zohaib\\GenerateFooter\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zohaib\\GenerateFooter\\GenerateFooterServiceProvider"
      ]
    }
  },
  "repositories": [
  {
    "type": "vcs",
    "url": "https://github.com/zohaib5laravel-code/generate-footer"
  }
],

  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10|^11"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
